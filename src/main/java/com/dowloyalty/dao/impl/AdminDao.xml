<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dowloyalty.dao.IAdminDao">
<select id="findAdminByUserId" parameterType="String" resultType="com.dowloyalty.entity.Admin">
	SELECT * FROM admin WHERE UserId=#{value} AND IsActive=1
</select>
</mapper>

